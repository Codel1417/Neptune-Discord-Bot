<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" 
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence                  
                                http://java.sun.com/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="psunit1">
 	
		<provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
		<class>neptune.storage.guild.guildObject</class>
        <class>neptune.storage.profile</class>
        <properties>
            <property name="hibernate.dialect" value="org.hibernate.dialect.H2Dialectt"/>
            <property name="hibernate.connection.url" value="jdbc:h2:~/data"/>
            <property name="hibernate.connection.driver_class" value="org.h2.Driver"/>
            <property name="hibernate.show_sql" value="true"/>
            <property name="hibernate.format_sql" value="true"/>
        </properties>
  </persistence-unit>
</persistence>